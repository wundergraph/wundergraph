{
  "name": "wundergraph-webhooks",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p wundergraph open",
    "wundergraph": "wunderctl up --debug",
    "generate": "wunderctl generate --debug",
    "open": "wait-on -d 500 http://localhost:9991 && open-cli http://localhost:9991/app/main/webhooks/Dragons",
    "wunderctl": "wunderctl",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "@wundergraph/sdk": "^0.117.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.15",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "ts-node": "^10.8.0",
    "typescript": "^4.8.2",
    "wait-on": "^6.0.1"
  }
}
