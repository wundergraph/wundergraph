{
  "name": "wundergraph-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p dev wundergraph open",
    "wundergraph": "wunderctl up",
    "open": "wait-on -d 500 http://localhost:9991 && open-cli http://localhost:3000",
    "build:wundergraph": "wunderctl generate",
    "build:next": "next build",
    "build": "npm run build:wundergraph && npm run build:next",
    "dev": "next dev",
    "check": "tsc --noEmit",
    "test": "jest",
    "test:playwright": "npx -- playwright test"
  },
  "dependencies": {
    "@wundergraph/nextjs": "^0.15.1",
    "@wundergraph/sdk": "^0.181.3",
    "graphql": "^16.3.0",
    "next": "^14.0.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "swr": "^2.2.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@playwright/test": "^1.28.1",
    "@types/node": "^17.0.15",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.6",
    "jest": "^29.6.1",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.8.0",
    "typescript": "^4.8.2",
    "wait-on": "^6.0.1"
  }
}
