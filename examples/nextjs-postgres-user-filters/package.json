{
  "name": "wundergraph-nextjs-postgres-prisma",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run start:services && wait-on -d 5000 tcp:54322 && npm run migrate init && run-p dev wundergraph seed",
    "start:services": "docker-compose up -d",
    "wundergraph": "wunderctl up --debug",
    "generate": "wunderctl generate",
    "open": "wait-on -d 500 http://localhost:9991 && open-cli http://localhost:3000",
    "seed": "wait-on -d 2000 http://localhost:9991 && ts-node ./seed/seed.ts && npm run open",
    "build": "next build",
    "dev": "next dev",
    "migrate": "prisma format && prisma migrate dev --name",
    "cleanup": "docker-compose down -v",
    "wunderctl": "wunderctl",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "@wundergraph/nextjs": "^0.6.1",
    "@wundergraph/sdk": "^0.124.1",
    "graphql": "^16.3.0",
    "next": "^12.1.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "swr": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.15",
    "@types/react": "^18.0.6",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "prisma": "^4.0.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.8.2",
    "wait-on": "^6.0.1"
  }
}
