{
  "name": "wundergraph-webhooks-orm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p wundergraph open",
    "wundergraph": "wunderctl up --debug",
    "build:wundergraph": "wunderctl generate",
    "build": "npm run build:wundergraph",
    "open": "wait-on -d 500 http://localhost:9991 && open-cli http://localhost:9991/webhooks/github",
    "wunderctl": "wunderctl",
    "check": "tsc --noEmit",
    "test": "jest"
  },
  "dependencies": {
    "@jest/globals": "^29.5.0",
    "@types/node-fetch": "^2.6.4",
    "@wundergraph/orm": "workspace:^0.0.3",
    "@wundergraph/sdk": "workspace:^0.158.1",
    "graphql": "^16.6.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.45",
    "node-fetch": "^2.6.11",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "wait-on": "^6.0.1"
  }
}
