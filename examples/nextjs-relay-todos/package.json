{
  "name": "wundergraph-nextjs-relay-todos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently \"wunderctl up --debug\" \"npm run relay\" \"next dev\"",
    "wundergraph": "wunderctl up --debug",
    "build": "wunderctl generate && next build",
    "lint": "next lint",
    "relay": "relay-compiler --watch"
  },
  "dependencies": {
    "@graphql-yoga/node": "^2.13.13",
    "@types/node": "18.15.13",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@wundergraph/react-relay": "^0.2.7",
    "@wundergraph/sdk": "^0.158.0",
    "autoprefixer": "10.4.14",
    "graphql": "^16.6.0",
    "next": "13.3.0",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-relay": "^15.0.0",
    "relay-runtime": "^15.0.0",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4"
  },
  "relay": {
    "src": "./src",
    "artifactDirectory": "./src/__relay__generated__",
    "language": "typescript",
    "schema": "./.wundergraph/generated/wundergraph.schema.graphql",
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**",
      "**/.wundergraph/generated/**"
    ],
    "persistConfig": {
      "file": "./.wundergraph/operations/relay/persisted.json"
    },
    "eagerEsModules": true
  },
  "devDependencies": {
    "@types/react-relay": "^14.1.3",
    "@types/relay-runtime": "^14.1.10",
    "babel-plugin-relay": "^15.0.0",
    "concurrently": "^8.0.1",
    "relay-compiler": "^15.0.0"
  }
}
