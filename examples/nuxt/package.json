{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "dev": "run-p dev:nuxt dev:wundergraph open",
    "dev:wundergraph": "wunderctl up",
    "dev:nuxt": "nuxt dev",
    "start": "wunderctl up",
    "open": "wait-on -d 500 http://localhost:9991 && open-cli http://localhost:3000",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "build:nuxt": "nuxt build",
    "build:wundergraph": "wunderctl generate --debug",
    "build": "run-s build:wundergraph build:nuxt",
    "check": "tsc --noEmit",
    "test": "vitest"
  },
  "devDependencies": {
    "@nuxt/devtools": "^0.3.1",
    "@nuxtjs/tailwindcss": "^6.6.5",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.3",
    "graphql": "^16.6.0",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.3.3",
    "open-cli": "^7.0.1",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "@tanstack/vue-query": "^4.27.0",
    "@wundergraph/sdk": "^0.180.1",
    "@wundergraph/vue-query": "^0.2.21",
    "unimport": "^3.0.6",
    "vitest": "^0.29.8",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "peerDependencies": {
    "@wundergraph/sdk": "^0.143.1"
  },
  "dependenciesMeta": {
    "@wundergraph/vue-query": {
      "injected": true
    }
  }
}
