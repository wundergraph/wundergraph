{
  "name": "wundergraph-schema-extension",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "run-p start:services start:server start:api",
    "start:api": "wait-on tcp:8081 && wunderctl up",
    "start:services": "docker-compose up -d",
    "start:server": "wait-on -d 5000 tcp:54322 && cd server && go run server.go",
    "build:wundergraph": "wunderctl generate",
    "build": "npm run build:wundergraph",
    "cleanup": "docker-compose down -v",
    "check": "tsc --noEmit",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@wundergraph/sdk": "^0.149.1",
    "graphql": "^16.3.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/node": "^14.14.37",
    "@types/node-fetch": "^2.6.2",
    "jest": "^29.3.1",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.2",
    "wait-on": "^6.0.1"
  }
}
