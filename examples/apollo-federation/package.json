{
  "name": "wundergraph-apollo-federation",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p -l start:services generate:api && run-p wundergraph start:nextjs open:browser",
    "postinstall": "cp -n .env.example .env",
    "start:services": "cd federation && docker-compose up -d",
    "wait-on:services": "wait-on http://localhost:4001 http://localhost:4002 http://localhost:4003 http://localhost:4004",
    "start:nextjs": "next dev",
    "open:browser": "wait-on -d 500 http://localhost:9991 && open-cli http://localhost:3000",
    "wundergraph": "wunderctl up --debug",
    "generate": "wunderctl generate --debug",
    "cleanup": "cd federation && docker-compose down -v",
    "check": "tsc --noEmit",
    "test": "jest"
  },
  "dependencies": {
    "@wundergraph/nextjs": "^0.7.0",
    "@wundergraph/sdk": "^0.125.0",
    "graphql": "^16.3.0",
    "next": "^12.1.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "swr": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.15",
    "@types/react": "^18.0.6",
    "@types/node-fetch": "^2.6.2",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "ts-node": "^10.8.0",
    "typescript": "^4.8.2",
    "wait-on": "^6.0.1",
    "jest": "^29.3.1",
    "@jest/globals": "^29.3.1",
    "ts-jest": "^29.0.3"
  }
}
