{
  "name": "wundergraph-swr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p dev wundergraph open",
    "wundergraph": "wunderctl up --debug",
    "open": "wait-on -d 500 http://localhost:9991 && open-cli http://localhost:3003",
    "build": "wunderctl generate --debug",
    "build:next": "next build",
    "dev": "next dev --port 3003",
    "check": "tsc --noEmit",
    "test": "jest"
  },
  "dependencies": {
    "@wundergraph/nextjs": "^0.7.0",
    "@wundergraph/sdk": "^0.125.0",
    "@wundergraph/swr": "^0.2.2",
    "graphql": "^16.3.0",
    "next": "^13.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "swr": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.15",
    "@types/react": "^18.0.6",
    "@types/node-fetch": "^2.6.2",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "ts-node": "^10.8.0",
    "typescript": "4.9.3",
    "wait-on": "^6.0.1",
    "@jest/globals": "^29.3.1",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3"
  }
}
