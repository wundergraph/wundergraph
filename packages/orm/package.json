{
  "name": "@wundergraph/orm",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "description": "WunderGraph ORM",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "type-test": "tsd"
  },
  "files": [
    "dist"
  ],
  "tsd": {
    "directory": "./test-d",
    "compilerOptions": {
      "typeRoots": []
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "dist"
    ],
    "testEnvironment": "node"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wundergraph/wundergraph.git"
  },
  "dependencies": {
    "@timkendall/tql": "1.0.0-rc.8",
    "graphql": "^16.6.0",
    "hotscript": "^1.0.10",
    "ix": "^5.0.0",
    "outvariant": "^1.4.0",
    "remeda": "^1.9.1",
    "ts-poet": "^6.4.1",
    "type-fest": "^3.5.2"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/node": "^18.15.10",
    "graphql-request": "^5.2.0",
    "jest": "^29.0.3",
    "ts-jest": "^29.0.1",
    "ts-toolbelt": "^9.6.0",
    "tsd": "^0.24.1",
    "typescript": "^4.9.4"
  },
  "homepage": "https://wundergraph.com",
  "author": {
    "name": "WunderGraph Maintainers",
    "email": "info@wundergraph.com"
  },
  "keywords": [
    "wundergraph",
    "orm",
    "graphql"
  ],
  "sideEffects": false
}
