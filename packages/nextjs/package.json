{
  "name": "@wundergraph/nextjs",
  "version": "0.14.2",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    },
    "./integration": {
      "import": "./dist/integration.mjs",
      "types": "./dist/integration.d.ts",
      "require": "./dist/integration.js"
    },
    "./template": {
      "import": "./dist/template.mjs",
      "types": "./dist/template.d.ts",
      "require": "./dist/template.js"
    },
    "./dist/template": {
      "import": "./dist/template.mjs",
      "types": "./dist/template.d.ts",
      "require": "./dist/template.js"
    }
  },
  "typesVersions": {
    "*": {
      "integration": [
        "./dist/integration"
      ],
      "template": [
        "./dist/template"
      ]
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/template.ts src/integration.ts --dts --format esm,cjs",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "description": "WunderGraph Next.js integration",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wundergraph/wundergraph.git"
  },
  "homepage": "https://wundergraph.com",
  "author": {
    "name": "WunderGraph Maintainers",
    "email": "info@wundergraph.com"
  },
  "bugs": {
    "url": "https://github.com/wundergraph/wundergraph/issues"
  },
  "devDependencies": {
    "@swc/core": "^1.3.14",
    "@swc/jest": "^0.2.23",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.32",
    "@types/node-fetch": "2.6.2",
    "@types/object-hash": "^1.3.4",
    "@types/react": "^18.0.15",
    "@wundergraph/sdk": "workspace:*",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "next": "^13.1.1",
    "nock": "^13.2.9",
    "node-fetch": "2.6.7",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "swr": "^2.2.0",
    "tsup": "^6.7.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "@wundergraph/sdk": ">=0.91.0",
    "next": "^12.1.0 || ^13.0.0",
    "react": "^16.8.0 || ^17.0.2 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.2 || ^18.0.0",
    "swr": "^2.2.0"
  },
  "dependencies": {
    "@wundergraph/protobuf": "workspace:^0.118.2",
    "@wundergraph/swr": "workspace:*",
    "handlebars": "^4.7.7",
    "object-hash": "^2.2.0",
    "react-ssr-prepass": "^1.5.0"
  },
  "sideEffects": false
}
