    "zod": "^3.20.2",
    "zod-to-json-schema": "^3.20.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "dist"
    ],
    "testEnvironment": "node"
  },
  "sideEffects": false
}
