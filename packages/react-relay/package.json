{
  "name": "@wundergraph/react-relay",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "description": "WunderGraph Relay Integration",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "tsd": {
    "directory": "tests"
  },
  "scripts": {
    "build": "tsc --noEmit && tsup src/index.ts --dts --format esm,cjs",
    "test": "jest --passWithNoTests && tsd"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wundergraph/wundergraph.git"
  },
  "homepage": "https://wundergraph.com",
  "author": {
    "name": "WunderGraph Maintainers",
    "email": "info@wundergraph.com"
  },
  "keywords": [
    "relay",
    "react",
    "react-relay",
    "nextjs",
    "wundergraph"
  ],
  "peerDependencies": {
    "react": "^16.9.0 || ^17 || ^18"
  },
  "dependencies": {
    "@wundergraph/sdk": "workspace:*",
    "react-relay": "^15.0.0",
    "relay-runtime": "^15.0.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.46",
    "@swc/jest": "^0.2.24",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.33",
    "@types/react-relay": "^14.1.3",
    "@types/relay-runtime": "^14.1.10",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "nock": "^13.2.9",
    "node-fetch": "^3.3.1",
    "tsd": "^0.24.1",
    "tsup": "^6.5.0",
    "typescript": "^5.0.3"
  }
}
