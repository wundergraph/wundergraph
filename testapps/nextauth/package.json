{
  "name": "wundergraph-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p dev wundergraph open",
    "wundergraph": "wunderctl up --logs",
    "generate": "wunderctl generate",
    "open": "wait-on -d 500 http://localhost:9991 && open-cli http://localhost:3000",
    "build": "pnpm generate && next build && pnpm check",
    "dev": "next dev",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "@wundergraph/nextjs": "workspace:*",
    "@wundergraph/sdk": "workspace:*",
    "graphql": "^16.3.0",
    "jose": "^4.11.1",
    "next": "^13.1.1",
    "next-auth": "^4.18.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "swr": "^2.0.0-rc.3"
  },
  "devDependencies": {
    "@types/node": "^17.0.15",
    "@types/react": "^18.0.6",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "ts-node": "^10.8.0",
    "typescript": "^4.8.2",
    "wait-on": "^6.0.1"
  }
}
