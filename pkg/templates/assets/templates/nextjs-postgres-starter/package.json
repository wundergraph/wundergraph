{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run start:services && wait-on -d 500 tcp:54322 && npm run migrate init && run-p dev wundergraph open",
    "start:services": "docker-compose up -d",
    "wundergraph": "wunderctl up --debug",
    "open": "wait-on -d 500 http://localhost:9991 && open-cli http://localhost:3000",
    "build": "next build",
    "dev": "next dev",
    "migrate": "prisma format && prisma migrate dev --name",
    "cleanup": "docker-compose down -v",
    "generate": "wunderctl generate --debug",
    "wunderctl": "wunderctl"
  },
  "dependencies": {
    "@wundergraph/sdk": "^0.98.1",
    "@wundergraph/nextjs": "^0.1.2",
    "graphql": "^16.3.0",
    "next": "^12.1.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.15",
    "@types/react": "^18.0.6",
    "npm-run-all": "^4.1.5",
    "prisma": "^2.28.0",
    "open-cli": "^7.0.1",
    "typescript": "^4.8.2",
    "wait-on": "^6.0.1"
  }
}
